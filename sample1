using AutoItX3Lib;

class Program
{
    static void Main(string[] args)
    {
        AutoItX3 autoIt = new AutoItX3();

        // Activate the login window (make sure to replace "LoginWindowTitle" with the actual title)
        autoIt.WinActivate("LoginWindowTitle");

        // Wait for the login window to appear (timeout in seconds)
        autoIt.WinWaitActive("LoginWindowTitle", "", 10);

        // Set the username (replace ControlID with the control identifier, such as control name or class)
        autoIt.ControlFocus("LoginWindowTitle", "", "[CLASS:Edit; INSTANCE:1]");
        autoIt.ControlSetText("LoginWindowTitle", "", "[CLASS:Edit; INSTANCE:1]", "YourUsername");

        // Set the password
        autoIt.ControlFocus("LoginWindowTitle", "", "[CLASS:Edit; INSTANCE:2]");
        autoIt.ControlSetText("LoginWindowTitle", "", "[CLASS:Edit; INSTANCE:2]", "YourPassword");

        // Click the Sign In button (replace ControlID with the control identifier)
        autoIt.ControlClick("LoginWindowTitle", "", "[CLASS:Button; INSTANCE:1]");

        // Optionally, wait for the next window or operation
        autoIt.WinWaitActive("NextWindowTitle", "", 10);
    }
}
