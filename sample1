using System;
using Excel = Microsoft.Office.Interop.Excel;

class Program
{
    static void Main()
    {
        // Initialize Excel application
        Excel.Application excelApp = new Excel.Application();
        
        // Make Excel visible (optional)
        excelApp.Visible = true;

        // Open the existing workbook
        string workbookPath = @"your_excel_file_path.xlsx";
        Excel.Workbook workbook = excelApp.Workbooks.Open(workbookPath);

        // Select the desired worksheet (1-based index, or by name)
        Excel.Worksheet worksheet = workbook.Sheets[1];

        try
        {
            // Set the header text in cell F3
            worksheet.Cells[3, 6].Value = "invoice"; // F is the 6th column

            // Save the workbook
            workbook.Save();
        }
        catch (Exception ex)
        {
            Console.WriteLine("Error: " + ex.Message);
        }
        finally
        {
            // Close the workbook and quit Excel application
            workbook.Close();
            excelApp.Quit();

            // Release COM objects
            ReleaseObject(worksheet);
            ReleaseObject(workbook);
            ReleaseObject(excelApp);
        }
    }

    private static void ReleaseObject(object obj)
    {
        try
        {
            System.Runtime.InteropServices.Marshal.ReleaseComObject(obj);
            obj = null;
        }
        catch (Exception ex)
        {
            obj = null;
            Console.WriteLine("Exception occurred while releasing object " + ex.ToString());
        }
        finally
        {
            GC.Collect();
        }
    }
}
