using FlaUI.Core.AutomationElements;
using FlaUI.Core.Conditions;
using FlaUI.Core.Definitions;
using FlaUI.Core.Tools;

// Attach to the running application
var app = Application.Attach("YourApplicationName");

// Find the main window
var mainWindow = app.GetMainWindow();

// Find the pane element
var pane = mainWindow.FindFirstChild(x => x.ByControlType(ControlType.Pane));

if (pane != null)
{
    // Access child elements within the pane
    var childElements = pane.FindAllChildren();

    foreach (var childElement in childElements)
    {
        // You can further filter child elements here based on their properties
        // For example, you can check if a child element is of type Button, Text, etc.
        // And then access their properties accordingly
    }
}
else
{
    Console.WriteLine("Pane element not found.");
}
