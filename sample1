using System;
using OpenQA.Selenium;
using OpenQA.Selenium.Chrome;

class Program
{
    static void Main()
    {
        // Initialize the Chrome WebDriver
        IWebDriver driver = new ChromeDriver();

        try
        {
            // Navigate to the URL containing the h5 tag
            driver.Navigate().GoToUrl("your_url_here");

            // Locate the parent div of the h5 tag with the text "Grant Access"
            IWebElement parentDiv = driver.FindElement(By.XPath("//h5[text()='Grant Access']/parent::div"));

            // Perform actions on the parent div if needed
            // Example: Print the text of the parent div
            Console.WriteLine("Parent div text: " + parentDiv.Text);

            Console.WriteLine("Parent div found successfully.");
        }
        catch (Exception ex)
        {
            Console.WriteLine("An error occurred: " + ex.Message);
        }
        finally
        {
            // Close the browser
            driver.Quit();
        }
    }
}
