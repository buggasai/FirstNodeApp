using AutoItX3Lib;

class Program
{
    static void Main(string[] args)
    {
        AutoItX3 autoIt = new AutoItX3();

        // Get the handle of the "Sign In" window (assuming it's custom with no title)
        string windowHandle = autoIt.WinGetHandle("[CLASS:Custom; TEXT:Sign in]");

        // Activate the window using the handle
        autoIt.WinActivate(windowHandle);

        // Wait until the custom "Sign In" window is active
        autoIt.WinWaitActive(windowHandle, "", 10);

        // Set the username (replace [NAME:Username] with correct identifier if found, or use position)
        autoIt.ControlSetText(windowHandle, "", "[NAME:Username]", "YourUsername");

        // Set the password (replace [NAME:Password] with correct identifier if found, or use position)
        autoIt.ControlSetText(windowHandle, "", "[NAME:Password]", "YourPassword");

        // Click the "Sign In" button (use a control identifier or the button's text if available)
        autoIt.ControlClick(windowHandle, "", "[TEXT:Sign in]");

        // Optionally wait for the next window or some feedback
        autoIt.WinWaitActive("NextWindowTitle", "", 10);
    }
}
