using Excel = Microsoft.Office.Interop.Excel;

class Program
{
    static void Main()
    {
        // Start Excel and open the workbook
        Excel.Application excelApp = new Excel.Application();
        Excel.Workbook workbook = excelApp.Workbooks.Open(@"C:\path\to\your\file.xlsx");
        Excel.Worksheet worksheet = workbook.Sheets["fakturadaten"];

        // Get the used range of the sheet
        Excel.Range usedRange = worksheet.UsedRange;

        // Get the last row number in the used range of Column C (3rd column)
        int lastRow = usedRange.Rows.Count;

        // Get the last two records from Column C
        string secondLastRecord = usedRange.Cells[lastRow - 1, 3].Value.ToString();
        string lastRecord = usedRange.Cells[lastRow, 3].Value.ToString();

        // Display the values
        Console.WriteLine("Second Last Record: " + secondLastRecord);
        Console.WriteLine("Last Record: " + lastRecord);

        // Clean up
        workbook.Close(false);
        excelApp.Quit();
    }
}
