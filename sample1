using System;
using HtmlAgilityPack;

class HtmlParserExample
{
    public void ExtractSpanText(string htmlFilePath)
    {
        // Load the HTML document
        var htmlDoc = new HtmlDocument();
        htmlDoc.Load(htmlFilePath);

        try
        {
            // Define the XPath to locate the span element
            string xpath = "//div[@id='content']//table//tbody//tr[2]//td//div//span";

            // Select the span node
            var spanNode = htmlDoc.DocumentNode.SelectSingleNode(xpath);

            // Check if the span node is found and get its inner text
            if (spanNode != null)
            {
                string spanText = spanNode.InnerText;
                Console.WriteLine("The text inside the span element is: " + spanText);
            }
            else
            {
                Console.WriteLine("Span element not found.");
            }
        }
        catch (Exception ex)
        {
            Console.WriteLine("Error: " + ex.Message);
        }
    }
}

class Program
{
    static void Main(string[] args)
    {
        HtmlParserExample example = new HtmlParserExample();
        example.ExtractSpanText("pat
