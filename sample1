from selenium import webdriver
from selenium.webdriver.common.by import By

# Setup WebDriver (example using Chrome)
driver = webdriver.Chrome()

# Navigate to the target webpage
driver.get("https://example.com/upload")

# Locate the file input element
file_input = driver.find_element(By.XPATH, "//input[@type='file']")

# Send the file path to the input element
file_input.send_keys("C:/Users/YourUsername/Desktop/Temp/yourfile.txt")

# Optionally, locate and click the upload button if required
upload_button = driver.find_element(By.XPATH, "//button[text()='Upload']")
upload_button.click()
