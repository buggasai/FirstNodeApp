using System;
using OpenQA.Selenium;
using OpenQA.Selenium.Chrome;

class Program
{
    static void Main()
    {
        // Initialize the Chrome WebDriver
        IWebDriver driver = new ChromeDriver();

        try
        {
            // Navigate to the URL containing the element
            driver.Navigate().GoToUrl("your_url_here");

            // Locate the element using its class name
            IWebElement toggleElement = driver.FindElement(By.ClassName("your_class_name"));

            // Perform the toggle action (e.g., click the element)
            toggleElement.Click();

            Console.WriteLine("Element toggled successfully.");
        }
        catch (Exception ex)
        {
            Console.WriteLine("An error occurred: " + ex.Message);
        }
        finally
        {
            // Close the browser
            driver.Quit();
        }
    }
}
