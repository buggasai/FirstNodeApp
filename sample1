using Excel = Microsoft.Office.Interop.Excel;

class Program
{
    static void Main()
    {
        Excel.Application excelApp = new Excel.Application();
        Excel.Workbook workbook = excelApp.Workbooks.Open(@"C:\path\to\your\workbook.xlsx");
        Excel.Worksheet worksheet = workbook.Sheets[1];

        // Define the range starting from row 9 downwards, extending to the last used column in your data
        Excel.Range sortRange = worksheet.Range["A9", worksheet.Cells[worksheet.Rows.Count, worksheet.Columns.Count].End(Excel.XlDirection.xlUp)];

        // Sort by Column B (smallest to largest)
        sortRange.Sort(
            Key1: sortRange.Columns[2],
            Order1: Excel.XlSortOrder.xlAscending,
            Header: Excel.XlYesNoGuess.xlNo
        );

        // Sort by Column H (smallest to largest)
        sortRange.Sort(
            Key1: sortRange.Columns[8],
            Order1: Excel.XlSortOrder.xlAscending,
            Header: Excel.XlYesNoGuess.xlNo
        );

        // Sort by Column E (oldest to newest)
        sortRange.Sort(
            Key1: sortRange.Columns[5],
            Order1: Excel.XlSortOrder.xlAscending,
            Header: Excel.XlYesNoGuess.xlNo
        );

        // Save and close the workbook
        workbook.Save();
        workbook.Close();
        excelApp.Quit();
    }
}
