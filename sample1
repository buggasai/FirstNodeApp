using OpenQA.Selenium;
using OpenQA.Selenium.Chrome;

class Program
{
    static void Main()
    {
        IWebDriver driver = new ChromeDriver();
        driver.Navigate().GoToUrl("your_webpage_url");

        try
        {
            // Find the input element using the constructed XPath
            IWebElement inputElement = driver.FindElement(By.XPath("//div[@id='content']//table//tr//td//table//tr[td/span[text()='sasaza']]/td[last()]/input[@type='text']"));
            if (inputElement != null)
            {
                Console.WriteLine("Input element found.");
                // Perform actions on the input element
                inputElement.SendKeys("Some text");
            }
            else
            {
                Console.WriteLine("Input element not found.");
            }
        }
        catch (NoSuchElementException)
        {
            Console.WriteLine("Element not found.");
        }
        finally
        {
            driver.Quit();
        }
    }
}
