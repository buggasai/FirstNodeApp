using OpenQA.Selenium;
using OpenQA.Selenium.Chrome;

class Program
{
    static void Main()
    {
        IWebDriver driver = new ChromeDriver();

        try
        {
            driver.Navigate().GoToUrl("http://example.com");

            // Wait until the alert is present
            IAlert alert = driver.SwitchTo().Alert();

            // Enter the username and password (assuming it's a JavaScript prompt)
            alert.SendKeys("your_username");
            alert.Accept();

            // If there's another prompt for password, repeat the process
            IAlert passwordAlert = driver.SwitchTo().Alert();
            passwordAlert.SendKeys("your_password");
            passwordAlert.Accept();

        }
        catch (Exception ex)
        {
            Console.WriteLine("An error occurred: " + ex.Message);
        }
        finally
        {
            driver.Quit();
        }
    }
}
