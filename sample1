using FlaUI.Core;
using FlaUI.Core.AutomationElements;
using FlaUI.UIA3;
using System;

class Program
{
    static void Main()
    {
        using (var app = Application.Launch("path_to_your_application.exe"))
        {
            using (var automation = new UIA3Automation())
            {
                // Get the main window
                var mainWindow = app.GetMainWindow(automation);

                // Find the custom "Sign In" pane
                var signInPane = mainWindow.FindFirstDescendant(cf => cf.ByName("Sign In"))?.AsPanel();

                if (signInPane != null)
                {
                    // Find the username and password input fields
                    var usernameInput = signInPane.FindFirstDescendant(cf => cf.ByAutomationId("usernameInputId"))?.AsTextBox();
                    var passwordInput = signInPane.FindFirstDescendant(cf => cf.ByAutomationId("passwordInputId"))?.AsTextBox();

                    // Enter the username and password
                    usernameInput?.Enter("your_username");
                    passwordInput?.Enter("your_password");

                    // Find and click the "Sign In" button
                    var signInButton = signInPane.FindFirstDescendant(cf => cf.ByName("Sign In"))?.AsButton();
                    signInButton?.Invoke();
                }
                else
                {
                    Console.WriteLine("Sign In pane not found.");
                }
            }
        }
    }
}
