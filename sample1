; Give time for the application to start
Sleep(5000)

; Focus on the window with the file dialog
WinActivate("Open") ; Adjust window title as needed

; Click on the 'File name:' input field
ControlClick("Open", "", "[CLASS:Edit; INSTANCE:1]")

; Wait a bit before typing the file path
Sleep(5000)

; Type the file path into the 'File name:' field
Send($desktopPath & "\\Temp\\Payroll Pay Element.xlsx")

; Wait again before sending Enter
Sleep(5000)

; Press Enter to confirm
Send("{ENTER}")
