import os
from selenium import webdriver
from selenium.webdriver.common.by import By

# Assuming you're using Chrome
driver = webdriver.Chrome()

# Navigate to the page with the file upload input
driver.get("https://example.com/upload")

# Locate the file input element (change the selector as per your web page)
file_input = driver.find_element(By.ID, "file-upload-input")

# Construct the file path to the Temp folder on the desktop
desktop_path = os.path.join(os.path.expanduser("~"), "Desktop")
temp_folder_path = os.path.join(desktop_path, "Temp")
file_to_upload = os.path.join(temp_folder_path, "yourfile.txt")  # Change the file name as needed

# Upload the file
file_input.send_keys(file_to_upload)
