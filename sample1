using System;
using HtmlAgilityPack;

class HtmlParserExample
{
    public void FindTdWithSpecificTexts(string htmlFilePath)
    {
        // Load the HTML document
        var htmlDoc = new HtmlDocument();
        htmlDoc.Load(htmlFilePath);

        try
        {
            // Define the XPath to locate the <td> element containing "pro5199" directly
            // and a <span> with "6310"
            string xpath = "//td[text()[normalize-space() = 'pro5199'] and .//span[normalize-space() = '6310']]";

            // Select the td node
            var tdNode = htmlDoc.DocumentNode.SelectSingleNode(xpath);

            // Check if the td node is found
            if (tdNode != null)
            {
                Console.WriteLine("Found td element:");
                Console.WriteLine(tdNode.OuterHtml);
            }
            else
            {
                Console.WriteLine("td element with 'pro5199' and span containing '6310' not found.");
            }
        }
        catch (Exception ex)
        {
            Console.WriteLine("Error: " + ex.Message);
        }
    }
}

class Program
{
    static void Main(string[] args)
    {
        HtmlParserExample example = new HtmlParserExample();
        example.FindTdWithSpecificTexts("path_to_your_html_file.html");
    }
}
