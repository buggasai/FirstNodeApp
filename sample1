using System;
using System.Runtime.InteropServices;
using Excel = Microsoft.Office.Interop.Excel;

class Program
{
    static void Main(string[] args)
    {
        string filePath = @"C:\path\to\your\file.xlsx";
        string sheetName = "SheetToDeleteData";

        Excel.Application excelApp = new Excel.Application();
        Excel.Workbook workbook = excelApp.Workbooks.Open(filePath);
        Excel.Worksheet worksheet = null;

        try
        {
            foreach (Excel.Worksheet sheet in workbook.Sheets)
            {
                if (sheet.Name == sheetName)
                {
                    worksheet = sheet;
                    break;
                }
            }

            if (worksheet != null)
            {
                worksheet.Cells.ClearContents(); // Clear all data in the worksheet
                workbook.Save();
                Console.WriteLine($"Data in sheet '{sheetName}' has been deleted.");
            }
            else
            {
                Console.WriteLine($"Sheet with name '{sheetName}' not found.");
            }
        }
        finally
        {
            workbook.Close(false);
            excelApp.Quit();

            // Release COM objects
            Marshal.ReleaseComObject(workbook);
            Marshal.ReleaseComObject(excelApp);
            if (worksheet != null) Marshal.ReleaseComObject(worksheet);
        }
    }
}
