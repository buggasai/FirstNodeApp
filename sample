using System;
using Microsoft.Office.Interop.Excel;

class Program
{
    static void Main()
    {
        string filePath = "YourFilePath.xlsx";  // Replace with the path to your Excel file
        DeleteRows(filePath, 1, 2);
    }

    static void DeleteRows(string filePath, int startRow, int endRow)
    {
        var excelApp = new Application();
        var workbook = excelApp.Workbooks.Open(filePath);
        var worksheet = (Worksheet)workbook.Sheets[1];

        // Calculate the last row to delete
        int lastRow = Math.Min(endRow, worksheet.Rows.Count);

        // Delete rows starting from the specified row until the end
        var range = worksheet.Rows[startRow + ":" + lastRow];
        range.Delete(XlDeleteShiftDirection.xlShiftUp);

        // Save and close the workbook
        workbook.Save();
        workbook.Close();
        excelApp.Quit();

        Console.WriteLine($"Rows {startRow} to {lastRow} deleted, and changes saved.");
    }
}
