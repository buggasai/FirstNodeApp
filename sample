using System;
using Microsoft.Office.Interop.Excel;

class Program
{
    static void Main()
    {
        string filePath = "YourFilePath.xlsx";  // Replace with the path to your Excel file
        InsertDataInFirstRow(filePath);
    }

    static void InsertDataInFirstRow(string filePath)
    {
        var excelApp = new Application();
        var workbook = excelApp.Workbooks.Open(filePath);
        var worksheet = (Worksheet)workbook.Sheets[1];

        // Insert data into the first row
        string[] rowData = { "Header1", "Header2", "Header3" };  // Replace with your data
        for (int col = 1; col <= rowData.Length; col++)
        {
            worksheet.Cells[1, col] = rowData[col - 1];
        }

        // Save and close the workbook
        workbook.Save();
        workbook.Close();
        excelApp.Quit();

        Console.WriteLine("Data inserted in the first row and changes saved.");
    }
}
